output "policy_arn" {
  description = "ARN of created IAM policy"
  value       = module.iam_policy.policy_arn
}

output "iam_user_name" {
  description = "Name of IAM User created"
  value       = module.iam_user.iam_user_name
}

output "access_key_id" {
  value = module.iam_access_key.access_key_id
}

output "secret_access_key" {
  value = module.iam_access_key.secret_access_key
}

output "kms_key_id" {
  value = module.kms.kms_key_id
}
